{
  "name": "@udistrital/root-config",
  "scripts": {
    "start": "webpack serve --port 4200 --env isLocal",
    "lint": "eslint src --ext js,ts,tsx",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "build": "concurrently npm:build:*",
    "build:webpack": "webpack --mode=production",
    "build:types": "tsc",
    "build:prod": "webpack --progress --mode production --no-devtool --env isProd",
    "build:test": "webpack --progress --mode production --no-devtool --env isDev"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.25.6",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "serve": "^14.2.3",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-config-single-spa-ts": "^4.1.4",
    "webpack-dev-server": "^5.1.0",
     "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@types/jest": "^27.0.1",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.2",
    "single-spa": "^5.9.3",
    "single-spa-layout": "^1.6.0"
  },
  "types": "dist/udistrital-root-config.d.ts"
}
